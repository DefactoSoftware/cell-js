    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/preset-env": "^7.8.4",
    "@babel/register": "^7.8.3",
    "babel-eslint": "^9.0.0",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-prettier": "^3.1.2",
    "jsdom": "^16.1.0",
    "mocha": "^7.0.1",
    "mocha-jsdom": "^2.0.0",
    "nyc": "^15.0.0",

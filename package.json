  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DefactoSoftware/cell-js/issues"
  },
  "homepage": "https://github.com/DefactoSoftware/cell-js#readme",
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^6.0.1",
    "eslint-plugin-prettier": "^3.1.0",
    "jsdom": "^15.1.1",
    "mocha": "^6.1.4",
    "mocha-jsdom": "^2.0.0",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "prettier-eslint": "^9.0.0",
